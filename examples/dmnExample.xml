	<BusinessKnowledgeModel name="AffordabilityCalculation" id="b_AffordabilityCalculation">
		<FunctionDefinition>
			<InformationItem name="MonthlyIncome" typeRef="feel:number" id="p_AffordabilityCalculation.MonthlyIncome"/>
			<InformationItem name="MonthlyRepayments" typeRef="feel:number" id="p_AffordabilityCalculation.MonthlyRepayments"/>
			<InformationItem name="MonthlyExpenses" typeRef="feel:number" id="p_AffordabilityCalculation.MonthlyExpenses"/>
			<InformationItem name="RiskCategory" typeRef="tns:tRiskCategory" id="p_AffordabilityCalculation.RiskCategory"/>
			<InformationItem name="RequiredMonthlyInstallment" typeRef="feel:number" id="p_AffordabilityCalculation.RequiredMonthlyInstallment"/>
			<Context>
				<contextEntry>
					<InformationItem name="DisposableIncome" typeRef="feel:number" id="v_AffordabilityCalculation.DisposableIncome"/>
					<LiteralExpression>
						<text>MonthlyIncome - (MonthlyExpenses + MonthlyRepayments)</text>
					</LiteralExpression>
				</contextEntry>
				<contextEntry>
					<InformationItem name="CreditContingencyFactor" typeRef="feel:number" id="v_AffordabilityCalculation.CreditContingencyFactor"/>
					<Invocation>
						<binding>
							<LiteralExpression>
								<text>RiskCategory</text>
							</LiteralExpression>
							<parameter href="#p_CreditContingencyFactorTable.RiskCategory"/>
						</binding>
					</Invocation>
				</contextEntry>
				<contextEntry>
					<InformationItem name="Affordability" typeRef="feel:boolean" id="v_AffordabilityCalculation.Affordability"/>
					<LiteralExpression>
						<text>If Disposable Income * Credit Contingency Factor &gt; Required Monthly Installment then true else false</text>
					</LiteralExpression>
				</contextEntry>
			</Context>
		</FunctionDefinition>
		<knowledgeRequirement>
			<requiredKnowledge href="#b_CreditContingencyFactorTable"/>
		</knowledgeRequirement>
	</BusinessKnowledgeModel>
	<BusinessKnowledgeModel name="CreditContingencyFactorTable" id="b_CreditContingencyFactorTable">
		<FunctionDefinition>
			<InformationItem name="RiskCategory" typeRef="tns:tRiskCategory" id="p_CreditContingencyFactorTable.RiskCategory"/>
			<DecisionTable hitPolicy="UNIQUE">
				<input>
					<inputExpression>
						<text>RiskCategory</text>
					</inputExpression>
					<inputValues>&quot;DECLINE&quot;,&quot;HIGH&quot;, &quot;LOW&quot;, &quot;MEDIUM&quot;, &quot;VERY LOW&quot;</inputValues>
				</input>
				<output/>
				<rule>
					<inputEntry>&quot;HIGH&quot;,&quot;DECLINE&quot;</inputEntry>
					<outputEntry>
						<text>0.6</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>&quot;MEDIUM&quot;</inputEntry>
					<outputEntry>
						<text>0.7</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>&quot;LOW&quot;,&quot;VERY LOW&quot;</inputEntry>
					<outputEntry>
						<text>0.8</text>
					</outputEntry>
				</rule>
			</DecisionTable>
		</FunctionDefinition>
	</BusinessKnowledgeModel>
	<BusinessKnowledgeModel name="EligibilityRules" id="b_EligibilityRules">
		<FunctionDefinition>
			<InformationItem name="Pre-bureauRiskCategory" typeRef="tns:tRiskCategory" id="p_EligibilityRules.Pre-bureauRiskCategory"/>
			<InformationItem name="Pre-bureauAffordability" typeRef="feel:boolean" id="p_EligibilityRules.Pre-bureauAffordability"/>
			<InformationItem name="Age" typeRef="feel:number" id="p_EligibilityRules.Age"/>
			<DecisionTable hitPolicy="PRIORITY">
				<input>
					<inputExpression>
						<text>Pre-bureau Risk Category</text>
					</inputExpression>
				</input>
				<input>
					<inputExpression>
						<text>Pre-bureau Affordability</text>
					</inputExpression>
				</input>
				<input>
					<inputExpression>
						<text>Age</text>
					</inputExpression>
				</input>
				<output/>
				<rule>
					<inputEntry>&quot;DECLINE&quot;</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>&quot;INELIGIBLE&quot;</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>-</inputEntry>
					<inputEntry>false</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>&quot;INELIGIBLE&quot;</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>&lt;18</inputEntry>
					<outputEntry>
						<text>&quot;INELIGIBLE&quot;</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>&quot;ELIGIBLE&quot;</text>
					</outputEntry>
				</rule>
			</DecisionTable>
		</FunctionDefinition>
	</BusinessKnowledgeModel>
	<BusinessKnowledgeModel name="BureauCallTypeTable" id="b_BureauCallTypeTable">
		<FunctionDefinition>
			<InformationItem name="Pre-bureauRiskCategory" typeRef="tns:tRiskCategory" id="p_BureauCallTypeTable.Pre-bureauRiskCategory"/>
			<DecisionTable hitPolicy="UNIQUE">
				<input>
					<inputExpression>
						<text>Pre-bureauRiskCategory</text>
					</inputExpression>
				</input>
				<output/>
				<rule>
					<inputEntry>&quot;HIGH&quot;,&quot;MEDIUM&quot;</inputEntry>
					<outputEntry>
						<text>&quot;FULL&quot;</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>&quot;LOW&quot;</inputEntry>
					<outputEntry>
						<text>&quot;MINI&quot;</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>&quot;VERY LOW&quot;,&quot;DECLINE&quot;</inputEntry>
					<outputEntry>
						<text>&quot;NONE&quot;</text>
					</outputEntry>
				</rule>
			</DecisionTable>
		</FunctionDefinition>
	</BusinessKnowledgeModel>
	<BusinessKnowledgeModel name="Pre-bureauRiskCategoryTable" id="b_Pre-bureauRiskCategoryTable">
		<FunctionDefinition>
			<InformationItem name="ExistingCustomer" typeRef="feel:boolean" id="p_Pre-bureauRiskCategoryTable.ExistingCustomer"/>
			<InformationItem name="ApplicationRiskScore" typeRef="feel:number" id="p_Pre-bureauRiskCategoryTable.ApplicationRiskScore"/>
			<DecisionTable hitPolicy="UNIQUE">
				<input>
					<inputExpression>
						<text>ExistingCustomer</text>
					</inputExpression>
				</input>
				<input>
					<inputExpression>
						<text>ApplicationRiskScore</text>
