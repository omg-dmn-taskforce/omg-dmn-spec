	<BusinessKnowledgeModel name="InstallmentCalculation" id="b_InstallmentCalculation">
		<FunctionDefinition>
			<InformationItem name="ProductType" typeRef="feel:string" id="p_InstallmentCalculation.ProductType"/>
			<InformationItem name="Rate" typeRef="feel:number" id="p_InstallmentCalculation.Rate"/>
			<InformationItem name="Term" typeRef="feel:number" id="p_InstallmentCalculation.Term"/>
			<InformationItem name="Amount" typeRef="feel:number" id="p_InstallmentCalculation.Amount"/>
			<Context>
				<contextEntry>
					<InformationItem name="MonthlyFee"/>
					<LiteralExpression>
						<text>If ProductType=&quot;STANDARD LOAN&quot; then 20.00 else if ProductType=&quot;SPECIAL LOAN&quot; then 25.00 else null</text>
					</LiteralExpression>
				</contextEntry>
				<contextEntry>
					<InformationItem name="MonthlyRepayment"/>
					<LiteralExpression>
						<text>(Amount *Rate/12) / (1 â€“ (1 + Rate/12)**-Term)</text>
					</LiteralExpression>
				</contextEntry>
				<contextEntry>
					<LiteralExpression>
						<text>MonthlyRepayment + MonthlyFee</text>
					</LiteralExpression>
				</contextEntry>
			</Context>
		</FunctionDefinition>
	</BusinessKnowledgeModel>
