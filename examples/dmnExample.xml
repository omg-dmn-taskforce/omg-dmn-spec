	<BusinessKnowledgeModel name="ApplicationRiskScoreModel" id="b_ApplicationRiskScoreModel">
		<FunctionDefinition>
			<InformationItem name="Age" typeRef="feel:number" id="p_ApplicationRiskScoreModel.Age"/>
			<InformationItem name="MaritalStatus" typeRef="feel:string" id="p_ApplicationRiskScoreModel.MaritalStatus"/>
			<InformationItem name="EmploymentStatus" typeRef="feel:string" id="p_ApplicationRiskScoreModel.EmploymentStatus"/>
			<DecisionTable hitPolicy="COLLECT" aggregation="SUM">
				<input>
					<inputExpression>
						<text>Age</text>
					</inputExpression>
					<inputValues>[18..120]</inputValues>
				</input>
				<input>
					<inputExpression>
						<text>MaritalStatus</text>
					</inputExpression>
					<inputValues>&quot;S&quot;,&quot;M&quot;</inputValues>
				</input>
				<input>
					<inputExpression>
						<text>EmploymentStatus</text>
					</inputExpression>
					<inputValues>&quot;UNEMPLOYED&quot;, &quot;EMPLOYED&quot;, &quot;SELF-EMPLOYED&quot;, &quot;STUDENT&quot;</inputValues>
				</input>
				<output/>
				<rule>
					<inputEntry>[18..21]</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>32</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>[22..25]</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
