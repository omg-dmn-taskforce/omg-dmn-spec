					<outputEntry>
						<text>36</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>&quot;STUDENT&quot;</inputEntry>
					<outputEntry>
