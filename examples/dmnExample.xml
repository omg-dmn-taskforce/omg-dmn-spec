					</outputEntry>
				</rule>
				<rule>
					<inputEntry>[26..35]</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>40</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>[36..49]</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>43</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>&gt;=50</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>48</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>-</inputEntry>
					<inputEntry>&quot;S&quot;</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>25</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>-</inputEntry>
					<inputEntry>&quot;M&quot;</inputEntry>
					<inputEntry>-</inputEntry>
					<outputEntry>
						<text>45</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
					<inputEntry>&quot;UNEMPLOYED&quot;</inputEntry>
					<outputEntry>
						<text>15</text>
					</outputEntry>
				</rule>
				<rule>
					<inputEntry>-</inputEntry>
					<inputEntry>-</inputEntry>
