	<Decision name="BureauCallType" id="d_BureauCallType">
		<InformationItem name="BureauCallType" id="v_BureauCallType" typeRef="tns:tBureauCallType"/>
		<informationRequirement>
			<requiredDecision href="#d_Pre-bureauRiskCategory"/>
		</informationRequirement>
		<knowledgeRequirement>
			<requiredKnowledge href="#b_BureauCallTypeTable"/>
		</knowledgeRequirement>
		<Invocation>
			<binding>
				<LiteralExpression>
					<text>Pre-bureauRiskCategory</text>
				</LiteralExpression>
				<parameter href="#p_Pre-bureauRiskCategory"/>
			</binding>
		</Invocation>
	</Decision>
	<Decision id="d_Strategy" name="Strategy">
		<description>3</description>
		<question>Is credit bureau call required?</question>
		<allowedAnswers>Yes (BUREAU) or No (DECLINE, THROUGH)</allowedAnswers>
		<InformationItem name="Strategy" id="v_Strategy" typeRef="tns:tStrategy"/>
		<informationRequirement>
			<requiredDecision href="#d_BureauCallType"/>
		</informationRequirement>
		<informationRequirement>
			<requiredDecision href="#d_Eligibility"/>
		</informationRequirement>
		<DecisionTable hitPolicy="UNIQUE" isComplete="true" isConsistent="true" preferedOrientation="Rule-as-Row">
			<input name="Eligibility" id="ix_Strategy.Eligibility">
				<inputExpression>
					<text>Eligibility</text>
				</inputExpression>
				<inputValues>"ELIGIBLE", "INELIGIBLE"</inputValues>
			</input>
			<input name="BureauCall" id="ix_Strategy.BureauCall">
				<inputExpression>
					<text>BureauCallType</text>
				</inputExpression>
				<inputValues>"FULL", "MINI", "NONE"</inputValues>
			</input>
			<output name="Strategy"/>
			<rule>
				<inputEntry inputRef="ix_Strategy.Eligibility">"INELIGIBLE"</inputEntry>
				<inputEntry inputRef="ix_Strategy.BureauCall">-</inputEntry>
				<outputEntry>
					<text>"DECLINE"</text>
				</outputEntry>
			</rule>
			<rule>
				<inputEntry inputRef="ix_Strategy.Eligibility">"ELIGIBLE"</inputEntry>
				<inputEntry inputRef="ix_Strategy.BureauCall">"FULL","MINI"</inputEntry>
				<outputEntry>
					<text>"BUREAU"</text>
				</outputEntry>
			</rule>
			<rule>
				<inputEntry inputRef="ix_Strategy.Eligibility">"ELIGIBLE"</inputEntry>
				<inputEntry inputRef="ix_Strategy.BureauCall">"NONE"</inputEntry>
				<outputEntry>
					<text>"THROUGH"</text>
				</outputEntry>
			</rule>
		</DecisionTable>
	</Decision>
